    pathPrepend(FHICL_FILE_PATH, ${MRB_SOURCE}/sbndqm/installations/sbn-nd)
    pathPrepend(FHICL_FILE_PATH, ${MRB_SOURCE}/sbndqm/installations/sbn-fd)
table_fragment_end


